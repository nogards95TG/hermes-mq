{
  "name": "hermes-mq",
  "version": "1.0.0",
  "description": "Modern, type-safe RabbitMQ client library for Node.js with RPC and Pub/Sub support",
  "author": "nogards95TG",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nogards95TG/hermes-mq.git"
  },
  "bugs": {
    "url": "https://github.com/nogards95TG/hermes-mq/issues"
  },
  "homepage": "https://github.com/nogards95TG/hermes-mq#readme",
  "keywords": [
    "rabbitmq",
    "amqp",
    "message-queue",
    "rpc",
    "pubsub",
    "publisher",
    "subscriber",
    "microservices",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "pre-push": "bash scripts/pre-push.sh",
    "publish:npm": "npm publish"
  },
  "dependencies": {
    "amqplib": "^0.10.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "prettier": "^3.2.4",
    "testcontainers": "^11.8.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
